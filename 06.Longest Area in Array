using System;

class LongestAreaInArray
{
    static void Main()
    {
        int n = int.Parse(Console.ReadLine());
        string[] word = new string[n];
        for (int i = 0; i < n; i++)
        {
            word[i]=Console.ReadLine();
        }
        string longWord = word[0];
        int []count = new int[n];
        count[0] = 1;
        for (int i = 1,j=0; i < n; i++)
        {
            if (j==n)
            {
                break;
            }
            if (longWord==word[i])
            {
                count[j] += 1;
            }
            else
            {
                longWord = word[i];
                j++;
                count[j] = 1;
            }
        }
        int lengthCount = count[0];
        longWord = word[0];
        for (int j = 0,i=1; j < n; j++,i++)
        {
            if (i==n)
            {
                break;  
            }
            if (lengthCount>=count[j])
            {
                continue;
            }
            else
            {
                lengthCount=count[j];
                longWord = word[i];
            }
        }
        if (lengthCount>1)
        {
            Console.WriteLine(lengthCount);
            for (int i = 0; i < lengthCount; i++)
            {
                Console.WriteLine(longWord);
            }
        }
        else
        {
            longWord = word[0];
            for (int i = 1; i < n; i++)
            {
                if (longWord.Length > word[i].Length)
                {
                    continue;
                }
                else
                {
                    longWord = word[i];
                }
            }
            Console.WriteLine(lengthCount);
            Console.WriteLine(longWord);
        }
    }
}

