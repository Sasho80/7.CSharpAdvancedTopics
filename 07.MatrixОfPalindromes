using System;

class MatrixOfPalindromes
{
    static void Main()
    {
        int row = int.Parse(Console.ReadLine());
        int col = int.Parse(Console.ReadLine());
        for (char i = 'a'; i < row+'a'; i++)
        {
            for (char j = i; j < col+i; j++)
            {
                
                Console.Write("{0}{1}{2}"+" ",i,j,i);   
            }
            Console.WriteLine();
        }
    }
}

